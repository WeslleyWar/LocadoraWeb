                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                        <ul class="navbar-nav flex-grow-1">
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Veiculo" asp-action="Index">Veiculos</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Usuario" asp-action="Login">Login</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Garagem" asp-action="Index">Garagem</a>
                            </li>

                            <li class="nav-item">
                                <div class="dropdown">
                                    <a class="nav-link text-dark" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        Contato
                                    </a>
                                    <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                        <a class="dropdown-item" href="#">Serviço de Atendimento ao Cliente - 0800 330 2020</a>
                                        <a class="dropdown-item" href="#">Via telefone ou whatsapp - 41 99777 6950</a>
                                        <a class="dropdown-item" href="#">Central de Reservas - 0800 330 3030</a>
                                        <a class="dropdown-item" href="#">Central de Assistência 24h - 0800 330 4040</a>
                                        <a class="dropdown-item" href="#">E-mail - Contato@VipCar.com.br</a>
                                    </div>


                                </div>
                            </li>
                    </div>
                </div>
            </nav>
        </header>
 



    </div>
    <div class="col-md-2"> </div>
</body>





@RenderBody()
            </div>
